{
  "name": "fe-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.2.2",
    "concurrently": "^7.6.0",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start":"concurrently \"npm run startBE\" \"npm run startFE\"",
    "startBE": "cd ../BE && concurrently \"npx tsc --watch\" \"nodemon dist/index.js\"",
    "startFE": "react-scripts start",
    "build":"npm run buildFE & npm run buildBE && npm run Copybuild",
    "buildFE": "react-scripts build",
    "buildBE": "cd ../BE && npm i && tsc",
    "Copybuild":"move build ../BE/dist/",
    "serve":"cd ../BE && node dist/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
